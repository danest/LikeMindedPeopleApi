<%= simple_form_for(@characteristic, html: {class: 'form-horizontal'}) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :key %>
    <%= f.input :attributeCategories %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn-primary' %>
		<%= link_to t('main.cancel'), characteristics_path, class: 'btn' %>
  </div>
<% end %>
